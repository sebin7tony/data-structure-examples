package com.java.bitMan;

public class PrintAllSubSets {
	
	public void printSubSets(int[] input){
		
		int len = input.length,temp;
		int currSubset = 2^len-1;
		
		while(currSubset > 0){
			
			System.out.print("(");
			temp = currSubset;
			for(int i=0;i< len ;i++){
				
				if(temp & 1){
					
					
				}
			}
		}
	}

}
