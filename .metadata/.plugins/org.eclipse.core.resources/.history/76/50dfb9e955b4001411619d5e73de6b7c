package com.ds.tree;

import java.util.ArrayList;
import java.util.List;

public class Queue {
	
	private List<Node> queue;
	private int size;
	private int top;
	
	public Queue(int size) {
		
		if(size == 0){
			
			System.out.println("Queue cannaot be made");
			return 0;
		}
		queue = new ArrayList<Node>();
		this.size = size;
		this.top = 0;
	}
	
	

}
